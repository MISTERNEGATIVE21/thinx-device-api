{
  "type": "thinx_server_config",
  "version" : "1",
  "public_url": "https://rtm.thinx.cloud",
  "api_url": "https://app.thinx.cloud",
  "acl_url": "https://rtm.thinx.cloud",
  "mailgun": {
    "api_key": "<api-key>",
    "base_url": "https://api.mailgun.net/v3/thinx.cloud",
    "domain": "thinx.cloud"
  },
  "redis": {
    "host": "thinx-redis",
    "db": 0,
    "port": 6379
  },
  "mqtt": {
    "server": "mqtt://mosquitto",
    "port": 1883
  },
  "slack" : {
    "client_id" : "000000000000.000000000000",
    "client_secret": "<not-configured>",
    "webhook": "<revoked>",
    "bot_token": "<not-configured>",
    "bot_topic": "thinx"
  },
  "use_sqreen": true,
  "port": 7442,
  "secure_port": 7443,
  "lambda": 7475,
  "data_root": "/mnt/data",
  "deploy_root": "/deploy",
  "build_root": "/repos",
  "ssh_keys": "/mnt/data/ssh_keys",
  "slack_webhook": "",
  "slack_bot_topic": "thinx",
  "ssl_key": "/mnt/data/ssl/privkey.pem",
  "ssl_cert": "/mnt/data/ssl/cert.pem",
  "client_user_agent": "THiNX-Client",
  "builder" : {
    "concurrency" : 2
  },
  "debug": {
    "device" : true,
    "deployment": true,
    "allow_http_login": true,
    "secrets": true
  },
  "strict_gdpr": true
}
